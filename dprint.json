{
  "lineWidth": 180,
  "indentWidth": 2,
  "includes": ["**/*.{ts,tsx,js,jsx,json,md}"],
  "excludes": [
    "dist",
    "npm/",
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.62.0.wasm",
    "https://plugins.dprint.dev/json-0.14.0.wasm",
    "https://plugins.dprint.dev/markdown-0.12.0.wasm"
  ]
}
