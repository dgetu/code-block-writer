{
  "lineWidth": 180,
  "indentWidth": 2,
  "includes": ["**/*.{ts,tsx,js,jsx,json,md}"],
  "excludes": [
    "dist",
    "npm/",
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.68.3.wasm",
    "https://plugins.dprint.dev/json-0.15.2.wasm",
    "https://plugins.dprint.dev/markdown-0.13.2.wasm"
  ]
}
